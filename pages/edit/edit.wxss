.list{
  padding-top:30rpx; 
  
}
.item {
  border-bottom:1px solid #e0e0e0;
  background-color: white;
}

.select-bless {
  position: relative;
  overflow: hidden;
}
.select-bless .box {
  margin: 35rpx ;
  float: right;
  background: #f67f7f;
  width: 28%;
  padding: 0 30rpx;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  color: white;
  border-radius: 99rpx;
}
.select-bless .box text{
  float: left;
}
.select-bless .box .iconfont {
font-size:40rpx;
float: left;
margin-left: 10rpx;
position: relative;
top: -5rpx;
}

.list.option .item {
  padding: 15rpx 5%;
}
.list.option .item .clear {
  clear: both;
}
.fixedBox {
  position: absolute;
  top: 0;
  z-index: 22;
}
